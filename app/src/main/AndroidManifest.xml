<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.atocash">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name="com.atocash.base.common.AtoCash"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppBaseTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="LockedOrientationActivity,UnusedAttribute">

        <activity
            android:name="com.atocash.common.activity.pendingExpenseReimDetails.PendingExpenseReimburseDetailsActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.activities.language.ChooseLanguageActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.common.activity.viewDoc.WebViewActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.common.activity.travelRequestDetails.TravelRequestDetailActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.activities.admin.travelRequest.ManageTravelRequestActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.common.activity.expenseReimburseDetails.ExpenseReimburseDetailsActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.common.activity.expenseReimburseListing.ExpenseReimburseListingActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.common.activity.cashAdvanceStatusDetails.CashAdvanceStatusDetailActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.common.activity.expenseReimburse.ManageExpenseReimburseActivity"
            android:windowSoftInputMode="adjustPan" />

        <activity
            android:name="com.atocash.activities.admin.cashAdvance.AdminCashAdvanceActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.activities.forgotPass.ForgotPasswordActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.activities.admin.expenseTypes.ManageExpenseTypeActivity"
            android:screenOrientation="portrait" />


        <activity
            android:name="com.atocash.activities.admin.jobRoles.ManageJobRolesActivity"
            android:screenOrientation="portrait" />


        <activity
            android:name="com.atocash.activities.admin.costCenter.ManageCostCenterActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.activities.resetPass.ResetPasswordActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.activities.employee.main.EmployeeMainActivity"
            android:label="@string/title_activity_employee_main"
            android:screenOrientation="portrait"
            android:theme="@style/AppBaseTheme.NoActionBar" />

        <activity
            android:name="com.atocash.activities.admin.main.AdminMainActivity"
            android:label="@string/title_activity_admin_main"
            android:screenOrientation="portrait"
            android:theme="@style/AppBaseTheme.NoActionBar" />

        <activity
            android:name="com.atocash.activities.main.MainActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.activities.splash.SplashActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.atocash.activities.login.LoginActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustResize" />

        <activity
            android:name="com.atocash.activities.notification.NotificationActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.activities.admin.department.AdminDepartmentActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.activities.admin.empTypes.ManageEmployeeActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.activities.admin.approval.ManageApprovalActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.activities.employee.expense.ManageEmpExpenseActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.activities.employee.advance.ManageEmpAdvanceActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.activities.admin.projects.ManageProjectsActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.activities.admin.common.ManageAdminActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.atocash.activities.admin.currency.ManageCurrencyActivity"
            android:screenOrientation="portrait" />

    </application>

</manifest>